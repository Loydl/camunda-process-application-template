{
  "title": "Find decision",
  "type": "object",
  "required": ["MANUAL_DECISION"],
  "properties": {
    "VIEW_ID": {
      "type": "string",
      "title": "Kunden-Nr.",
      "default": "${customerId}"
    },
    "VIEW_NAME": {
      "type": "string",
      "title": "Name",
      "default": "${fullName}"
    },
    "VIEW_AMOUNT": {
      "type": "string",
      "title": "Kreditsumme",
      "default": "${amountInEuro}"
    },
    "VIEW_PERSONALITY": {
      "type": "string",
      "title": "Person",
      "default": "${personality}"
    },
    "VIEW_RATING": {
      "type": "string",
      "title": "Rating",
      "default": "${rating}"
    },
    "MANUAL_DECISION": {
      "type": "string",
      "title": "Rating",
      "enum": [
        "ACCEPT",
        "REJECT"
      ]
    }
  }
}